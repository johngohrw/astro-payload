---
import type { BlockType } from "@astro-default/blocks/_blockTypes";
import { allBlockRenderersMap } from "@astro-default/blocks/rendererMap";

interface Props {
  block: BlockType;
}

const { block } = Astro.props;

const Renderer = allBlockRenderersMap[block.blockType];
---

{Renderer && <Renderer block={block} />}
