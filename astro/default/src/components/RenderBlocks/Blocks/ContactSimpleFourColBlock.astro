---
import ContactSimpleFourCol from "@astro-default/components/TailwindPlus/Contact/ContactSimpleFourCol.astro";
import type { BlockTypeOf } from "../types";

const { block: incomingBlock } = Astro.props;

const block: BlockTypeOf<"contactSimpleFourCol"> = incomingBlock;
---

<ContactSimpleFourCol
  title={block.title}
  subtitle={block.subtitle ?? ""}
  offices={block.offices.map((o) => ({
    name: o.name,
    lines: (o.lines ?? []).map((l) => l.line),
  }))}
/>
